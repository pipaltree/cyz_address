{
  "name": "pipaltree/cyz-address",
  "type": "typo3-cms-extension",
  "license": "MIT",
  "description": "Makes it possible to use fluid templates with tt_address",
  "authors": [
    {
      "name": "Luca Kredel",
      "email": "luca.kredel@cyperfection.de"
    },
    {
      "name": "Markus Hackel",
      "email": "markus.hackel@gmail.com"
    }
  ],
  "require": {
    "typo3/cms-core": "^7.6 || ^8.7"
  },
  "autoload": {
    "psr-4": {
      "Cyz\\CyzAddress\\": "Classes/"
    }
  },
  "replace": {
    "cyz_address": "self.version",
    "typo3-ter/cyz_address": "self.version"
  }
}
